library(shiny)
library(bslib)

ui <- page_sidebar(
  title = "title panel",
  sidebar = sidebar("sidebar"),
  "main contents"
)

server <- function(input, output) {

   }

shinyApp(ui = ui, server = server)

#' Add two numbers
#' @param a
#' @param b
#' @details adds a to b.
add_a_and_b <- function(a, b) {
  a + b
}

roxygen2::roxygenize()
library(intakegui)
?add_a_and_b

